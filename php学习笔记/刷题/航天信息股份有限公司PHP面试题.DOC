\lang2052\f2\'b4\'f0\'a3\'ba\lang1033\f0 //1)\par
function get_ext1($file_name)\par
\{\par
        return strrchr($file_name, '.');\par
\}\par
\par
//2)\par
function get_ext2($file_name)\par
\{\par
        return substr($file_name, strrpos($file_name, '.'));\par
\}\par
\par
//3)\par
function get_ext3($file_name)\par
\{\par
        return array_pop(explode('.', $file_name));\par
\}\par
\par
//4)\par
function get_ext4($file_name)\par
\{\par
        $p = pathinfo($file_name);\par
        return $p['extension'];\par
\}\par
\par
//5)\par
function get_ext5($file_name)\par
\{\par
        return strrev(substr(strrev($file_name), 0, strpos(strrev($file_name), '.')));\par
\}\par
