\b0 <?php\par
class Child\{\par
 public $no;  //\lang2052\f2\'d0\'a1\'ba\'a2\'b1\'e0\'ba\'c5\lang1033\f0\par
 public $next; //\lang2052\f2\'cf\'c2\'d2\'bb\'b8\'f6\'d0\'a1\'ba\'a2\lang1033\f0\par
\par
//\lang2052\f2\'b9\'b9\'b3\'c9\'bb\'b7\'d0\'ce\'c1\'b4\'b1\'ed\lang1033\f0  \lang2052\f2\'b5\'c8\'d3\'da\'d0\'a1\'ba\'a2\'ce\'a7\'b3\'c9\'d2\'bb\'b8\'f6\'d4\'b2\'c8\'a6\lang1033\f0\par
 function addChild($n)\{\par
 \par
   for($i=0;$i<$n;$i++)\{\par
        $child= new self();\par
                $child->no=$i+1;\par
           if($i==0)\{\par
                 $head = $child;\par
                 $child->next=$head;\par
                 $cur=$head;\par
           \}else\{\par
             $cur->next=$child;\par
\par
                 $child->next=$head;\par
                 $cur=$cur->next;\par
       \} \par
   \}\par
    return $head;\par
 \}\par
 //\lang2052\f2\'bf\'aa\'ca\'bc\'cc\'df\'d0\'a1\'ba\'a2\lang1033\f0\par
 function countChild($head)\{\par
    $cur=$head;\par
        while($cur->next!=$head)\{  \par
              $cur=$cur->next;\par
        \}\par
    //\lang2052\f2\'b5\'b1\'c7\'b0\'d0\'a1\'ba\'a2\'b5\'c8\'cd\'b7\'d0\'a1\'ba\'a2\lang1033\f0  \lang2052\f2\'be\'cd\'d6\'bb\'ca\'a3\'cf\'c2\'d2\'bb\'b8\'f6\'d0\'a1\'ba\'a2\lang1033\f0\par
        while($cur!=$head)\{\par
           \par
           //\lang2052\f2\'ca\'fd\'b5\'bd\lang1033\f0 3\lang2052\f2\'d0\'a1\'ba\'a2\'b3\'f6\'c8\'a6\lang1033\f0\par
           for($i=0;$i<3-1;$i++)\{\par
             $cur=$cur->next;\par
                 $head=$head->next;\par
           \par
           \} \par
            echo '\lang2052\f2\'b3\'f6\'c8\'a6\'d0\'a1\'ba\'a2\lang1033\f0 '.$head->no.'<br />';\par
            $head=$head->next;\par
                $cur->next=$head;\par
        \par
        \}\par
    echo '\lang2052\f2\'d7\'ee\'ba\'f3\'b5\'c4\'d0\'a1\'ba\'a2\lang1033\f0 '.$cur->no;\par
 \}\par
\}\par
\par
$child =new Child();\par
$b=$child->addChild(10);\par
$child->countChild($b);\par
\par
?>\par
